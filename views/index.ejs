                                    <div style="width: 50%;" class="card card-primary">
                                        <div class="card-header">
                                            <h3 class="card-title">Update Product</h3>
                                        </div>
                                        <form>
                                            <div class="card-body">
                                                <div>
                                                    <img id="up-img-preview" src="" alt="" style="max-width: 50%; margin-bottom: 20px;">
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-id-barang">ID Barang</label>
                                                    <div class="form-control" id="up-id-barang" placeholder="Nama Barang" required></div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-nama-barang">Nama Barang</label>
                                                    <input type="text" class="form-control" id="up-nama-barang" placeholder="Nama Barang" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-harga-beli">Harga Beli</label>
                                                    <input type="text" class="form-control" id="up-harga-beli" placeholder="Harga Beli" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-harga-jual">Harga Jual</label>
                                                    <input type="text" class="form-control" id="up-harga-jual" placeholder="Harga Jual" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-stock-barang">Stock</label>
                                                    <input type="text" class="form-control" id="up-stock-barang" placeholder="Stock" required>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-gambar-barang">Nama Gambar Barang</label>
                                                    <div style="height: 90px;" class="db-row form-control" id="up-gambar-barang"></div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="up-ganti-gambar-barang">Ganti Gambar Barang</label>
                                                    <div class="input-group">
                                                        <div class="custom-file">
                                                            <input type="file" class="custom-file-input" id="up-ganti-gambar-barang" required>
                                                            <label class="custom-file-label" for="up-ganti-gambar-barang">Choose file</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <button id="btn-update-reset" type="reset" class="btn btn-primary">Reset</button>
                                        </form>
                                        <div class="card-footer">
                                        <button id="btn-update-submit" class="btn btn-primary">Submit</button>
                                        <button id="btn-update-close" class="btn btn-primary">Close</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- view product -->
                        <div id="view-product" class="card">
                            <div class="card-header">
