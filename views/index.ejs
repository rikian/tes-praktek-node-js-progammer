                        <!-- add product -->
                        <div id="add-product" class="card">
                            <div class="card-header">
                                <h3 class="card-title">Add Product</h3>
                            </div>
                            <div class="card-body">
                                <div id="product" class="dataTables_wrapper dt-bootstap4">
                                    <div style="width: 50%;" class="card card-primary">
                                        <div class="card-header">
                                            <h3 class="card-title">Tambah Product</h3>
                                        </div>
                                        <form>
                                            <div class="card-body">
                                                <div id="con-img-preview"><img id="img-preview" src="" alt="" style="max-width: 50%;"></div>
                                                <div class="form-group">
                                                <label for="nama-barang">Nama Barang</label>
                                                <input type="text" class="form-control" id="nama-barang" placeholder="Nama Barang" required>
                                                </div>
                                                <div class="form-group">
                                                <label for="harga-beli">Harga Beli</label>
                                                <input type="text" class="form-control" id="harga-beli" placeholder="Harga Beli" required>
                                                </div>
                                                <div class="form-group">
                                                <label for="harga-jual">Harga Jual</label>
                                                <input type="text" class="form-control" id="harga-jual" placeholder="Harga Jual" required>
                                                </div>
                                                <div class="form-group">
                                                <label for="stock-barang">Stock</label>
                                                <input type="text" class="form-control" id="stock-barang" placeholder="Stock" required>
                                                </div>
                                                <div class="form-group">
                                                <label for="gambar-barang">Gambar Barang</label>
                                                <div class="input-group">
                                                    <div class="custom-file">
                                                    <input type="file" class="custom-file-input" id="gambar-barang" required>
                                                    <label class="custom-file-label" for="gambar-barang">Choose file</label>
                                                    </div>
                                                </div>
                                                </div>
                                            </div>
                                        
                                            <button id="btn-add-reset" type="reset" class="btn btn-primary">Reset</button>
                                        </form>
                                        <div class="card-footer">
                                            <button id="btn-add-submit" class="btn btn-primary">Submit</button>
                                            <button id="btn-add-close" class="btn btn-primary">Close</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- update product -->
                        <div id="update-product" class="card">
                            <div class="card-header">
                            <h3 class="card-title">Update Product</h3>
                            </div>
                            <div class="card-body">
